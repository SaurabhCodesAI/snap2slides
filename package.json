  "description": "AI-powered presentation generator that transforms images into professional slides",
  "author": {
    "name": "Saurabh Pareek",
    "email": "saurabhpareek228@gmail.com",
    "url": "https://github.com/SaurabhCodesAI"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaurabhCodesAI/snap2slides-hackathon.git"
  },
  "homepage": "https://github.com/SaurabhCodesAI/snap2slides-hackathon#readme",
  "bugs": {
    "url": "https://github.com/SaurabhCodesAI/snap2slides-hackathon/issues",
    "email": "saurabhpareek228@gmail.com"
  },
  "keywords": [
    "ai",
    "presentations",
    "nextjs",
    "typescript",
    "gemini",
    "slides",
    "hackathon"
  ],
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev",
    "dev:port": "node server.js",
    "dev:clean": "rimraf .next && node server.js",
    "build": "next build",
    "start": "next start",
    "start:custom": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage dist",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "preview": "npm run build && npm run start",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "validate": "npm run type-check && npm run lint && npm run test:ci",
    "port:check": "node scripts/port-diagnostics.js check",
    "port:suggest": "node scripts/port-diagnostics.js suggest",
    "port:monitor": "node scripts/port-diagnostics.js monitor",
    "port:diagnose": "node scripts/port-diagnostics.js full"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.5.0",
    "@google-cloud/storage": "^7.16.0",
    "@google/generative-ai": "^0.15.0",
    "@heroicons/react": "^2.2.0",
    "@types/lodash": "^4.17.20",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.9",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.8.0",
    "nanoid": "^5.1.5",
    "next": "^14.2.30",
    "pptxgenjs": "^3.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-dropzone": "^4.2.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
